{
  "name": "remark-git-contributors",
  "version": "4.0.1",
  "description": "remark plugin to generate a list of Git contributors",
  "license": "MIT",
  "keywords": [
    "contributors",
    "github",
    "inject",
    "markdown",
    "mdast",
    "plugin",
    "remark",
    "remark-plugin",
    "unified"
  ],
  "repository": "remarkjs/remark-git-contributors",
  "bugs": "https://github.com/remarkjs/remark-git-contributors/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": {
    "name": "Vincent Weevers",
    "email": "mail@vincentweevers.nl",
    "github": "vweevers",
    "twitter": "vweevers"
  },
  "contributors": [
    {
      "name": "Vincent Weevers",
      "email": "mail@vincentweevers.nl",
      "github": "vweevers",
      "twitter": "vweevers"
    },
    {
      "name": "Titus Wormer",
      "email": "tituswormer@gmail.com",
      "github": "wooorm",
      "twitter": "wooorm"
    }
  ],
  "sideEffects": false,
  "type": "module",
  "exports": "./index.js",
  "files": [
    "formatters.d.ts",
    "formatters.js",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/mdast": "^4.0.0",
    "contributors-from-git": "^1.0.0",
    "dlv": "^1.0.0",
    "load-plugin": "^5.0.0",
    "mdast-util-heading-range": "^4.0.0",
    "parse-author": "^2.0.0",
    "remark-contributors": "^7.0.0",
    "vfile": "^6.0.0",
    "vfile-find-up": "^7.0.0",
    "vfile-message": "^4.0.0"
  },
  "devDependencies": {
    "@types/dlv": "^1.0.0",
    "@types/node": "^20.0.0",
    "@types/parse-author": "^2.0.0",
    "c8": "^8.0.0",
    "prettier": "^3.0.0",
    "remark": "^15.0.0",
    "remark-cli": "^11.0.0",
    "remark-gfm": "^4.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "semver": "^7.0.0",
    "tmpgen": "^1.0.0",
    "type-coverage": "^2.0.0",
    "type-fest": "^4.0.0",
    "typescript": "^5.0.0",
    "xo": "^0.56.0"
  },
  "scripts": {
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . --frail --output --quiet && prettier . --log-level warn --write && xo --fix",
    "prepack": "npm run build && npm run format",
    "test": "npm run build && npm run format && npm run test-coverage",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --100 --reporter lcov npm run test-api"
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-wooorm",
      "./index.js"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "ignoreCatch": true,
    "strict": true
  },
  "xo": {
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "no-await-in-loop": "off"
        }
      }
    ],
    "prettier": true,
    "rules": {
      "unicorn/prefer-string-replace-all": "off"
    }
  }
}
