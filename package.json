{
  "name": "remark-git-contributors",
  "version": "1.1.0",
  "description": "remark plugin to inject Git contributors into a Markdown table",
  "license": "MIT",
  "keywords": [
    "unified",
    "remark",
    "plugin",
    "mdast",
    "markdown",
    "contributors",
    "github",
    "inject"
  ],
  "repository": "remarkjs/remark-git-contributors",
  "bugs": "https://github.com/remarkjs/remark-git-contributors/issues",
  "author": {
    "name": "Vincent Weevers",
    "email": "mail@vincentweevers.nl",
    "github": "vweevers",
    "twitter": "vweevers"
  },
  "contributors": [
    {
      "name": "Vincent Weevers",
      "email": "mail@vincentweevers.nl",
      "github": "vweevers",
      "twitter": "vweevers"
    },
    {
      "name": "Titus Wormer",
      "email": "tituswormer@gmail.com",
      "github": "wooorm",
      "twitter": "wooorm"
    }
  ],
  "files": [
    "formatters.js",
    "index.js"
  ],
  "dependencies": {
    "contributors-from-git": "^1.0.0",
    "deep-dot": "0.0.2",
    "mdast-util-heading-range": "^2.1.0",
    "parse-author": "^2.0.0",
    "remark-contributors": "^4.0.0",
    "resolve": "^1.8.1"
  },
  "devDependencies": {
    "nyc": "^14.0.0",
    "remark": "^10.0.0",
    "remark-cli": "^6.0.0",
    "remark-preset-wooorm": "^5.0.0",
    "standard": "^12.0.0",
    "tape": "^4.0.0",
    "tmpgen": "^1.0.0"
  },
  "scripts": {
    "format": "standard --fix && remark *.md -qfo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm",
      "."
    ]
  }
}
